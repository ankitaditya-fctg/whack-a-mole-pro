{
  "name": "whack-a-mole-pro",
  "displayName": "Whack-a-Mole Pro",
  "description": "Interactive whack-a-mole game with design patterns and GitHub integration",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "whack-a-mole-pro.openGame",
        "title": "Open Whack-a-Mole Game"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "jest",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/node": "^20.0.0",
    "@vscode/webview-ui-toolkit": "^1.2.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "ts-loader": "^9.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.33"
  }
}
